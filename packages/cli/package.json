{
  "name": "base44",
  "version": "0.0.1",
  "description": "Base44 CLI - Unified interface for managing Base44 applications",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "lint": "echo 'Linting CLI package'"
  },
  "keywords": [
    "base44",
    "cli",
    "command-line"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@base44/cli-core": "workspace:*",
    "@clack/prompts": "^0.11.0",
    "chalk": "^5.6.2",
    "commander": "^12.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
