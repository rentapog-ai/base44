{
  "name": "base44",
  "version": "0.0.1",
  "description": "Base44 CLI - Unified interface for managing Base44 applications",
  "type": "module",
  "main": "./dist/cli/index.js",
  "types": "./dist/cli/index.d.ts",
  "bin": "./dist/cli/index.js",
  "exports": {
    ".": {
      "types": "./dist/cli/index.d.ts",
      "default": "./dist/cli/index.js"
    }
